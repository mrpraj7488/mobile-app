{
  "expo": {
    "name": "VidGro",
    "slug": "vidgro-watch",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "vidgro",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "icon": "assets/images/icon.png",
    "splash": {
      "image": "assets/images/flash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF",
      "hideExponentText": true
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vidgro.watch",
      "buildNumber": "1",
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": false,
          "NSExceptionDomains": {
            "youtube.com": {
              "NSExceptionAllowsInsecureHTTPLoads": false,
              "NSExceptionMinimumTLSVersion": "TLSv1.2"
            },
            "supabase.co": {
              "NSExceptionAllowsInsecureHTTPLoads": false,
              "NSExceptionMinimumTLSVersion": "TLSv1.2"
            }
          }
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "assets/images/icon.png",
        "backgroundColor": "#0F172A"
      },
      "package": "com.zozo1.vidgrowatch",
      "versionCode": 1,
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "WAKE_LOCK"
      ],
      "blockedPermissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "READ_CONTACTS",
        "WRITE_CONTACTS",
        "READ_SMS",
        "SEND_SMS"
      ]
    },
    "web": {
      "bundler": "webpack",
      "output": "single"
    },
    "assetBundlePatterns": [
      "assets/images/**"
    ],
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "@react-native-google-signin/google-signin"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
